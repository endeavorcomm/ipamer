<div class="row mt-4">
  <div class="col s10 offset-s1">
    <table>
      <thead>
        <tr>
          <th>Name</th>
          <th>IP Addresses</th>
          <th>Description</th>
        </tr>
      </thead>
      <tbody>
        {{#each customer}}
        <tr>
          <td>{{this.name}}</td><td>{{this.addresses}}</td><td>{{this.description}}</td>
        </tr>
        {{/each}}
      </tbody>
    </table>
  </div>
</div>